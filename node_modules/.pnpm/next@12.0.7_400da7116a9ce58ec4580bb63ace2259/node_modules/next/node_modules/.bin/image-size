#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/image-size@1.0.0/node_modules/image-size/bin/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/image-size@1.0.0/node_modules/image-size/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/image-size@1.0.0/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules:/Users/adamrneary/github/node_modules:/Users/adamrneary/node_modules:/Users/node_modules:/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/next@12.0.7_400da7116a9ce58ec4580bb63ace2259/node_modules/next/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/next@12.0.7_400da7116a9ce58ec4580bb63ace2259/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/image-size@1.0.0/node_modules/image-size/bin/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/image-size@1.0.0/node_modules/image-size/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/image-size@1.0.0/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules:/Users/adamrneary/github/node_modules:/Users/adamrneary/node_modules:/Users/node_modules:/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/next@12.0.7_400da7116a9ce58ec4580bb63ace2259/node_modules/next/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/next@12.0.7_400da7116a9ce58ec4580bb63ace2259/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../image-size@1.0.0/node_modules/image-size/bin/image-size.js" "$@"
else
  exec node  "$basedir/../../../../../image-size@1.0.0/node_modules/image-size/bin/image-size.js" "$@"
fi
