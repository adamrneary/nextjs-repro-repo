#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/he@1.2.0/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules:/Users/adamrneary/github/node_modules:/Users/adamrneary/node_modules:/Users/node_modules:/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node-html-parser@1.4.9/node_modules/node-html-parser/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node-html-parser@1.4.9/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/he@1.2.0/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules:/Users/adamrneary/github/node_modules:/Users/adamrneary/node_modules:/Users/node_modules:/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node-html-parser@1.4.9/node_modules/node-html-parser/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node-html-parser@1.4.9/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
else
  exec node  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
fi
