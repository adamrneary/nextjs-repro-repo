#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/browserslist@4.16.6/node_modules/browserslist/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/browserslist@4.16.6/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules:/Users/adamrneary/github/node_modules:/Users/adamrneary/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/browserslist@4.16.6/node_modules/browserslist/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/browserslist@4.16.6/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules/.pnpm/node_modules:/Users/adamrneary/github/oom-repro-repo/node_modules:/Users/adamrneary/github/node_modules:/Users/adamrneary/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
